# Railway fills DATABASE_URL when you attach a Postgres plugin
# DATABASE_URL=postgresql://user:pass@host:port/db

# Optional: restrict API with a token
EDITOR_AUTH_TOKEN=replace-me

# Optional: limit CORS
CORS_ORIGIN=*

